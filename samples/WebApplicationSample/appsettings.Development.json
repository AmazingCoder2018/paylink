{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },

  // 开发环境配置

  "WeChatPay": {
    // 应用号
    // 微信公众平台AppId / 微信开放平台AppId / 微信小程序AppId / 企业微信CorpId
    "AppId": "",

    // 微信支付商户号
    "MchId": "",

    // API秘钥，不是APIv3密钥
    "Key": "",

    // API证书 .p12
    "Certificate": "",

    // RSA公钥 (目前仅"企业付款到银行卡API"使用，调用"获取RSA加密公钥API"即可获取)
    "RsaPublicKey": ""
  },
  "Alipay": {

    // 注意: 
    // 示例默认的加签方式为公钥证书，默认调用 CertificateExecuteAsync 方法 执行请求。
    // 不使用公钥证书，除了遵守配置选项规则外，CertificateExecuteAsync 方法 需要改成 ExecuteAsync 方法。

    // 应用ID
    "AppId": "",

    // RSA 支付宝公钥，加签方式为公钥证书时，留空
    "AlipayPublicKey": "",

    // RSA 应用私钥，加签方式为公钥证书时，为证书私钥
    "AppPrivateKey": "",

    // 网关地址，默认正式环境
    "ServerUrl": "https://openapi.alipay.com/gateway.do",

    // 签名类型
    "SignType": "RSA2",

    // 应用公钥证书
    // 加签方式为公钥证书时，必填
    "AppCert": "",

    // 支付宝公钥证书
    // 加签方式为公钥证书时，必填
    "AlipayPublicCert": "",

    // 支付宝根证书
    // 加签方式为公钥证书时，必填
    "RootCert": ""
  }
}
